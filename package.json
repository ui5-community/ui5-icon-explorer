{
	"name": "de.kernich.icon.preview",
	"version": "1.0.1",
	"description": "Icon Explorer which extends ui5-fontawesome-lib",
	"author": "Mario Kernich",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/ui5-community/ui5-icon-explorer.git"
	},
	"keywords": [
		"ui5",
		"fontawesome",
		"icon",
		"explorer"
	],
	"scripts": {
		"clean": "rimraf dist coverage",
		"build": "ui5 build --clean-dest",
		"build-for-deploy": "ui5 build --clean-dest && mv dist/index-cdn.html dist/index.html",
		"build:opt": "ui5 build self-contained --clean-dest --all",
		"start": "ui5 serve --port 8082 -o index.html",
		"check:ts": "tsc --noEmit",
		"check:lint": "eslint webapp",
		"check:ui5": "ui5lint",
		"check:all": "npm run check:ts && npm run check:lint && npm run check:ui5",
		"fix:lint": "eslint webapp --fix",
		"prepare": "husky"
	},
	"devDependencies": {
		"@commitlint/cli": "latest",
		"@commitlint/config-conventional": "latest",
		"@openui5/types": "^1.138.0",
		"@ui5/cli": "latest",
		"@ui5/linter": "latest",
		"eslint": "^9.12.0",
		"globals": "^15.11.0",
		"husky": "^9.1.7",
		"rimraf": "latest",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.8.1",
		"ui5-middleware-livereload": "latest",
		"ui5-middleware-servestatic": "latest",
		"ui5-test-runner": "latest",
		"ui5-tooling-modules": "latest",
		"ui5-tooling-transpile": "latest"
	},
	"dependencies": {
		"ui5-fontawesome-lib": "^1.0.0"
	}
}
